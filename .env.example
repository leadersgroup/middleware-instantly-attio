# HubSpot Configuration
HUBSPOT_API_KEY=your_hubspot_api_key_here
HUBSPOT_API_URL=https://api.hubapi.com

# Instantly Configuration
INSTANTLY_API_KEY=your_instantly_api_key_here
INSTANTLY_API_URL=https://api.instantly.ai/api/v2

# HubSpot Owner ID (default: 85461012 for admin@50deeds.com)
HUBSPOT_INSTANTLY_OWNER_ID=85461012

# HubSpot Sequence IDs (Get these by running: node get-hubspot-sequences.js)
HUBSPOT_SEQUENCE_NEW_LEAD=
HUBSPOT_SEQUENCE_TRIAL=
HUBSPOT_SEQUENCE_CUSTOMER=
HUBSPOT_SEQUENCE_REENGAGE=

# Webhook Secrets
INSTANTLY_WEBHOOK_SECRET=
HUBSPOT_WEBHOOK_SECRET=

# Server Configuration
PORT=3001
NODE_ENV=development
SYNC_ENABLED=true
LOG_LEVEL=info
